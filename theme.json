{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"color": {
			"custom": false,
			"defaultPalette": false,
			"defaultGradients": false,
			"customDuotone": false,
			"customGradient": false,
			"palette": [
				{
					"slug": "foreground",
					"color": "#0A0A0A",
					"name": "Foreground (Dark)"
				},
				{
					"slug": "background",
					"color": "#FEFEFE",
					"name": "Background (Light)"
				},
				{
					"slug": "white",
					"color": "#FEFEFE",
					"name": "White"
				},
				{
					"slug": "black",
					"color": "#0A0A0A",
					"name": "Black"
				},
				{
					"slug": "cyan",
					"color": "#3DC5E0",
					"name": "Cyan"
				},
				{
					"slug": "red",
					"color": "#D3012A",
					"name": "Red"
				},
				{
					"slug": "yellow",
					"color": "#F4B438",
					"name": "Yellow"
				},
				{
					"slug": "lavender",
					"color": "#C0A9FF",
					"name": "Lavender"
				},
				{
					"slug": "navy",
					"color": "#022645",
					"name": "Navy"
				},
				{
					"slug": "blue",
					"color": "#054296",
					"name": "Blue"
				},
				{
					"slug": "jade",
					"color": "#0B616D",
					"name": "Jade"
				},
				{
					"slug": "violet",
					"color": "#5F3DC4",
					"name": "Violet"
				},
				{
					"slug": "newsprint",
					"color": "#DBD6D2",
					"name": "Newsprint"
				},
				{
					"slug": "cerulean",
					"color": "#3A7BE4",
					"name": "Cerulean"
				}
			],
			"gradients": [],
			"link": true
		},
		"custom": {
			"border-radius": {
				"tiny": "3px",
				"small": "8px",
				"medium": "12px",
				"large": "50%"
			},
			"border-width": {
				"tiny": "1px",
				"small": "2px",
				"medium": "3px",
				"large": "4px"
			},
			"box-shadow": {
				"1": "0px 2px 8px rgba(33, 33, 33, 0.12)",
				"2": "0px 3px 10px rgba(33, 33, 33, 0.25)"
			},
			"color": {
				"link": "#0000EE",
				"neutral-50": "#FAFAFA",
				"neutral-100": "#F5F5F5",
				"neutral-200": "#EEEEEE",
				"neutral-300": "#E0E0E0",
				"neutral-400": "#BDBDBD",
				"neutral-500": "#9E9E9E",
				"neutral-600": "#757575",
				"neutral-700": "#616161",
				"neutral-800": "#424242",
				"neutral-900": "#212121"
			},
			"layout": {
				"content": "800px",
				"wide": "1200px",
				"sidebar": "336px",
				"page": "var(--wp--custom--layout--content)",
				"padding": "16px",
				"block-gap": "16px",
				"block-gap-large": "40px"
			},
			"letter-spacing": {
				"none": "normal",
				"tight": "-0.01em",
				"loose": "0.05em",
				"looser": "0.1em",
				"h1": "-0.56px",
				"h2": "-0.48px",
				"h3": "-0.36px",
				"h4": "2.475px",
				"button": "0.32px"
			},
			"line-height": {
				"tiny": 1.1,
				"small": 1.2,
				"medium": 1.4,
				"normal": 1.6,
				"h1": "60px",
				"h2": "41px",
				"h3": "41px",
				"h4": "22.5px",
				"h5": "45px",
				"h6": "38px"
			}
		},
		"spacing": {
			"blockGap": false,
			"margin": false,
			"padding": false,
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"typography": {
			"dropCap": false,
			"lineHeight": false,
			"customFontSize": false,
			"letterSpacing": false,
			"fontFamilies": [
				{
					"fontFamily": "\"Oswald\", sans-serif",
					"name": "Oswald",
					"slug": "oswald"
				},
				{
					"fontFamily": "\"DM Sans\", sans-serif",
					"name": "DM Sans",
					"slug": "dm-sans"
				},
				{
					"fontFamily": "\"DM Mono\", monospace",
					"name": "DM Mono",
					"slug": "dm-mono"
				}
			],
			"fontSizes": [
				{
					"name": "H1",
					"size": "56px",
					"slug": "h1"
				},
				{
					"name": "H2",
					"size": "48px",
					"slug": "h2"
				},
				{
					"name": "H3",
					"size": "36px",
					"slug": "h3"
				},
				{
					"name": "H4",
					"size": "16.5px",
					"slug": "h4"
				},
				{
					"name": "H5",
					"size": "38px",
					"slug": "h5"
				},
				{
					"name": "H6",
					"size": "30px",
					"slug": "h6"
				},
				{
					"name": "Large Paragraph",
					"size": "30px",
					"slug": "large"
				},
				{
					"name": "Regular Paragraph",
					"size": "18px",
					"slug": "medium"
				},
				{
					"name": "Small Paragraph",
					"size": "16.5px",
					"slug": "small"
				},
				{
					"name": "Button/Tiny",
					"size": "16px",
					"slug": "tiny"
				}
			]
		},
		"layout": {
			"contentSize": "var(--wp--custom--layout--content)",
			"wideSize": "var(--wp--custom--layout--wide)"
		},
		"border": {
			"color": false,
			"radius": false,
			"style": false,
			"width": false
		},
		"blocks": {
			"core/button": {
				"border": {
					"radius": false
				},
				"color": {
					"text": true,
					"background": true
				}
			},
			"core/group": {
				"color": {
					"background": true,
					"text": true
				}
			},
			"core/heading": {
				"color": {
					"text": true,
					"background": true
				}
			},
			"core/paragraph": {
				"color": {
					"text": true,
					"background": true
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--custom--color--link)"
				}
			},
			"button": {
				"color": {
					"background": "var(--wp--preset--color--black)",
					"text": "var(--wp--preset--color--white)"
				},
				"border": {
					"width": "4px",
					"style": "solid",
					"color": "var(--wp--preset--color--black)",
					"radius": "0px"
				},
				"spacing": {
					"padding": "12px 24px"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dm-sans)",
					"fontSize": "16.5px",
					"fontWeight": "900",
					"lineHeight": "22.5px",
					"letterSpacing": "2.475px",
					"textTransform": "uppercase"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--black)",
						"text": "var(--wp--preset--color--white)"
					}
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)",
					"fontWeight": "400"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)",
					"fontSize": "56px",
					"fontWeight": "400",
					"lineHeight": "60px",
					"letterSpacing": "-0.56px"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)",
					"fontSize": "48px",
					"fontWeight": "400",
					"lineHeight": "41px",
					"letterSpacing": "-0.48px"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)",
					"fontSize": "36px",
					"fontWeight": "400",
					"lineHeight": "41px",
					"letterSpacing": "-0.36px"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dm-sans)",
					"fontSize": "16.5px",
					"fontWeight": "900",
					"lineHeight": "22.5px",
					"letterSpacing": "2.475px",
					"textTransform": "uppercase"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dm-mono)",
					"fontSize": "38px",
					"fontWeight": "400",
					"lineHeight": "45px"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dm-sans)",
					"fontSize": "30px",
					"fontWeight": "400",
					"lineHeight": "38px"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--custom--layout--block-gap)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--dm-sans)",
			"lineHeight": "25px",
			"fontSize": "18px"
		}
	}
}
